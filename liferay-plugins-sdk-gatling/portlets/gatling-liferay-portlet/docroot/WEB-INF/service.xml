<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.liferay.sample">
	<author>sana</author>
	<namespace>StressTool</namespace>
	<entity name="Simulation" local-service="true" remote-service="false">
		<column name="id" type="long" primary="true"></column>
		<column name="name" type="String"></column>
	</entity>
	<entity name="Scenario" local-service="true" remote-service="false">
		<column name="id" type="long" primary="true"></column>
		<column name="name" type="String"></column>
		<column name="id_simulation" type="long"></column>
	</entity>
	<entity name="Request" local-service="true" remote-service="false">
		<column name="id" type="long" primary="true"></column>
		<column name="id_scenario" type="long"></column>
		<column name="url" type="String"></column>
		<column name="charge" type="long"></column>
	</entity>

</service-builder>